{
  "name": "hello-world",
  "version": "1.0.0",
  "description": "Simple Hello World Website",
  "main": "index.js",
  "scripts": {
    "dev": "rm -rf dist/* && parcel src/index.html -d dist",
    "build": "npm run delete-build && parcel build src/index.html --no-source-maps --public-url . -d .",
    "delete-build": "rm -f *.png && rm -f *.jpg && rm -f *.jpeg && rm -f *.ico && rm -f *.html && rm -f *.css && rm -f *.map && rm -f style.*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weekian/hello-world.git"
  },
  "author": "Wee Kian",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/weekian/hello-world/issues"
  },
  "homepage": "https://github.com/weekian/hello-world#readme",
  "devDependencies": {
    "autoprefixer": "^9.4.7",
    "cssnano": "^4.1.8",
    "postcss-modules": "^1.4.1",
    "sass": "^1.16.1"
  },
  "dependencies": {}
}
